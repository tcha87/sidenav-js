{
  "name": "chialab-sidenav",
  "version": "1.0.1",
  "description": "A mobile friendly sidenav implementation.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chialab/sidenav-js"
  },
  "main": "index.js",
  "build": {
    "exportDefault": true,
    "bundleName": "sidenav.js",
    "globalName": "SideNav",
    "globalDeps": {
      "dna/components": "DNA",
      "chialab/gestures": "Gestures"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "node ./node_modules/es6-workflow/scripts/build.js",
    "test": "node ./node_modules/es6-workflow/scripts/test.js",
    "doc": "node ./node_modules/es6-workflow/scripts/doc.js",
    "add-editorconfig": "node ./node_modules/es6-workflow/scripts/add-editorconfig.js",
    "add-eslint": "node ./node_modules/es6-workflow/scripts/add-eslint.js",
    "add-travis": "node ./node_modules/es6-workflow/scripts/add-travis.js",
    "codecov": "node ./node_modules/es6-workflow/scripts/codecov.js",
    "coveralls": "node ./node_modules/es6-workflow/scripts/coveralls.js",
    "test-ci": "node ./node_modules/es6-workflow/scripts/test-ci.js"
  },
  "author": "Chialab <dev@chialab.io> (http://www.chialab.com)",
  "devDependencies": {
    "es6-workflow": "^0.4.6"
  },
  "dependencies": {
    "chialab-gestures": "^1.1.0",
    "dna-components": "^1.2.0"
  }
}
